pirarie.Name = "Music"
pirarie.SoundId = "rbxassetid://1847452015"
pirarie.Pitch = 0
pirarie.Looped = true
pirarie.RollOffMaxDistance = 10000
pirarie.RollOffMinDistance = 10
pirarie:Play()
