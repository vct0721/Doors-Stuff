-- Setting Up Functions
local function OpenDoor()
    local door = workspace.CurrentRooms[roomValue]:FindFirstChild("Door")
    
    if door and door:FindFirstChild("ClientOpen") then
        door.ClientOpen:FireServer()
    end
end

