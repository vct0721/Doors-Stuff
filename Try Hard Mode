-- Door New Materials
ReplicatedStorage.GameData.LatestRoom.Changed:Connect(function()
	if IsMines then
		SyncHelper:deltaWait(0.0005)
		local door = Workspace.CurrentRooms[ReplicatedStorage.GameData.LatestRoom.Value]:FindFirstChild("Door")
		if door then
			door.Sign.Material = Enum.Material.CorrodedMetal
		end
	elseif IsBackHotel then
		SyncHelper:deltaWait(0.0005)
		local door = Workspace.CurrentRooms[GameData.LatestRoom.Value]:FindFirstChild("Door")
		if door then
			door.Material = Enum.Material.Brick
		end
	end
end)

