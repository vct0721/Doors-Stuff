-- Rush & Ambush Nightmare
coroutine.wrap(function()
    game.ReplicatedStorage.GameData.LatestRoom.Changed:Connect(function()
        wait(1)

        local rushMoving = game.Workspace:FindFirstChild("RushMoving")
        if rushMoving then
            local assetIdRush = "rbxassetid://12111964011"
            local successRush, assetRush = pcall(function()
                return game:GetService("InsertService"):LoadAsset(tonumber(assetIdRush:match("%d+")))
            end)

            if successRush and assetRush and assetRush:IsA("Model") then
                local newRush = assetRush:FindFirstChildWhichIsA("Model")
                if newRush then
                    local oldRushNew = rushMoving:FindFirstChild("RushNew")
                    if oldRushNew then
                        oldRushNew:Destroy()
                    end
                    newRush.Name = "RushNew"
                    newRush.Parent = rushMoving

                    if newRush:FindFirstChild("PlaySound") then
                        newRush.PlaySound.PlaybackSpeed = 0.05
                    end

                    if newRush:FindFirstChild("Footsteps") then
                        newRush.Footsteps.PlaybackSpeed = 0.1
                    end
                end
            end
        end

        local ambushMoving = game.Workspace:FindFirstChild("AmbushMoving")
        if ambushMoving then
            local assetIdAmbush = "rbxassetid://12158222955"
            local successAmbush, assetAmbush = pcall(function()
                return game:GetService("InsertService"):LoadAsset(tonumber(assetIdAmbush:match("%d+")))
            end)

            if successAmbush and assetAmbush and assetAmbush:IsA("Model") then
                local newAmbush = assetAmbush:FindFirstChildWhichIsA("Model")
                if newAmbush then
                    local oldAmbushNew = ambushMoving:FindFirstChild("RushNew")
                    if oldAmbushNew then
                        oldAmbushNew:Destroy()
                    end
                    newAmbush.Name = "RushNew"
                    newAmbush.Parent = ambushMoving

                    if newAmbush:FindFirstChild("PlaySound") then
                        newAmbush.PlaySound.PlaybackSpeed = 0.4
                    end

                    if newAmbush:FindFirstChild("Footsteps") then
                        newAmbush.Footsteps.PlaybackSpeed = 0.5
                    end
                end
            end
        end
    end)
end)()


-- Deep Hotel Room 52 Spawn
coroutine.wrap(function()
    game.ReplicatedStorage.GameData.LatestRoom.Changed:Connect(function()
        if game.ReplicatedStorage.GameData.LatestRoom.Value == 52 then

            function GitAud(soundlink, filename)
                local url = soundlink
                local FileName = filename
                if not isfile(FileName..".mp3") then
                    writefile(FileName..".mp3", game:HttpGet(url))
                end
                return (getcustomasset or getsynasset)(FileName..".mp3")
            end

            function CustomGitSound(soundlink, vol, filename)
                local IntroSound = Instance.new("Sound")
                IntroSound.SoundId = GitAud(soundlink, filename)
                IntroSound.Parent = workspace
                IntroSound.Volume = vol
                IntroSound:Play()
            end

            CustomGitSound("https://github.com/Kotyara19k-Doorsspawner/Ping-s-Dumbass-projects-/raw/main/Dark-Depths_Entrencebetter.mp3", 1, "DeepHotel")

            -- Set up horror atmosphere
            Lighting.Ambient = Color3.new(0, 0, 0)
            Lighting.Brightness = 0.1
            Lighting.FogEnd = 60
            Lighting.FogColor = Color3.new(0, 0, 0)

            -- Show intro text
            local shut = game.Players.LocalPlayer.PlayerGui.MainUI.MainFrame.IntroText
            local intro = shut:Clone()
            intro.Parent = game.Players.LocalPlayer.PlayerGui.MainUI
            intro.Name = "DeepHotel"
            intro.Visible = true
            intro.Text = "The Deep Hotel"
            intro.TextTransparency = 0
            local underline = UDim2.new(1.1, 0, 0.015, 6)
            game.TweenService:Create(intro.Underline, TweenInfo.new(3), {Size = underline}):Play()
            wait(7)
            game.TweenService:Create(intro.Underline, TweenInfo.new(1.3), {Size = UDim2.new(0.95, 0, 0.015, 6)}):Play()
            wait(1)
            game.TweenService:Create(intro.Underline, TweenInfo.new(2), {ImageTransparency = 1}):Play()
            game.TweenService:Create(intro, TweenInfo.new(2), {TextTransparency = 1}):Play()
            game.TweenService:Create(intro.Underline, TweenInfo.new(7), {Size = UDim2.new(0, 0, 0.015, 6)}):Play()
            wait(2.3)
            intro.Visible = false

            -- Lights Remove
            coroutine.wrap(function()
                while true do
				wait(0.0005)
				game.workspace.CurrentRooms[tostring(game:GetService("ReplicatedStorage").GameData.LatestRoom.Value)]:WaitForChild("Assets").Chandelier:Destroy()
                game.workspace.CurrentRooms[tostring(game:GetService("ReplicatedStorage").GameData.LatestRoom.Value)]:WaitForChild("Assets").Light_Fixtures:Destroy()
				    end
                end
            end)()

            -- A-90
            coroutine.wrap(function()
                while true do
                    wait(29.50)
                    if not IsSeekChase then
                        game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
                        wait(0)
                        loadstring(game:HttpGet("https://pastefy.app/K2TbsMWk/raw"))()
                    end
                end
            end)()
			
-- The Watcher
