-- Ambience 2

coroutine.wrap(function()

	if game:GetService("ReplicatedStorage").GameData.Floor.Value == "Hotel" then

		game.ReplicatedStorage.GameData.LatestRoom.Changed:Connect(function()

			SyncHelper:deltaWait(0.0005)

			function ReplaceAudGit(GithubSnd)
				local url = GithubSnd
				local soundFileName = "CustomSound.mp3"

				if not isfile(soundFileName) then
					writefile(soundFileName, game:HttpGet(url))
				end

				return (getcustomasset or getsynasset)(soundFileName)
			end	

			game.Workspace.CurrentRooms["50"]:FindFirstChild("FigureSetup").Ambience.SoundId = ReplaceAudGit("https://github.com/Kotyara19k-Doorsspawner/Ping-s-Dumbass-projects-/raw/refs/heads/main/Figure%20Ambience.mp3")
			game.Workspace.CurrentRooms["50"]:FindFirstChild("FigureSetup").Ambience.Volume = "0.7"
			game.Workspace.CurrentRooms["51"]:FindFirstChild("FigureSetup").AmbienceEnd.SoundId = ReplaceAudGit("https://github.com/Kotyara19k-Doorsspawner/Ping-s-Dumbass-projects-/raw/refs/heads/main/Figure%20End.mp3")
			game.Workspace.CurrentRooms["51"]:FindFirstChild("FigureSetup").AmbienceEnd.Volume = "0.5"

		end)

		--[[ ]]--

	end

end)()

